{
  "listeners": {
    "*:443": {
      "pass": "routes",
      "tls": {
        "certificate": ["certbot1"]
      }
    },
    "*:80": {
      "pass": "routes"
    }
  },
  "routes": [
     {
      "match": {
        "host": "bialet.org"
      },
      "action": {
	"proxy": "http://127.0.0.1:7001"
      }
    },
     {
      "match": {
        "host": ["dev.ar", "*.dev.ar"]
      },
      "action": {
	"proxy": "http://127.0.0.1:7002"
      }
    }
  ]
}
